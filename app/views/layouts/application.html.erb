<!DOCTYPE html>
<html>
	<head>
	  <title>Nested Pages</title>
	  <%= stylesheet_link_tag    "application" %>
	  <%= javascript_include_tag "application" %>
	  <%= csrf_meta_tags %>
	</head>
	<body>
		<div class="container">
	    <% flash.each do |key, value| %>
	      <%= content_tag(:div, value, class: "flash_#{key}") %>
	    <% end %>
	    <%= yield %>
	    <%= debug(params) if Rails.env.development? %>
	  </div>
	</body>
</html>
